{
  "name": "movierama",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "serve-dev": "webpack-dev-server --mode=development --config webpack.config.js",
    "build:webpack": "webpack --mode=production -p --config webpack.config.js",
    "serve-prod": "serve dist",
    "heroku-postbuild": "npm run build:webpack -- --env.apiKey=${apiKey}",
    "start": "npm run serve-prod",
    "test": "jasmine-ts --config=spec/support/jasmine.json",
    "test-watch": "nodemon --ext ts --exec 'npm run test'",
    "testWithCoverage": "nyc jasmine-ts --config=spec/support/jasmine.json && nyc report",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tslint-fix": "tslint --fix src/**/*.ts",
    "prettier-check": "prettier --check {src,spec}/**/*.{js,ts,css,less,scss,json,md} *.{js,ts,css,less,scss,json,md} .*.{js,ts,css,less,scss,json,md}",
    "prettier-fix": "prettier --write {src,spec}/**/*.{js,ts,css,less,scss,json,md} *.{js,ts,css,less,scss,json,md} .*.{js,ts,css,less,scss,json,md}",
    "fix": "npm run prettier-fix && npm run tslint-fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/jasmine": "^3.3.13",
    "@types/jsdom": "^12.2.4",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "html-webpack-plugin": "^5.5.0",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "jsdom": "^15.1.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "css-loader": "^3.0.0",
    "serve": "^11.0.2",
    "style-loader": "^0.23.1"
  }
}
